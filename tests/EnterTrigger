(function () {
    var Portal = function() {};

    Portal.prototype = {
        // Метод, который вызывается при входе в объект
        enterEntity: function (id) {
            print("Player entered the entity with ID: " + id);
        },

        // Метод, который вызывается при выходе из объекта
        leaveEntity: function (id) {
            print("Player left the entity with ID: " + id);
        }
    };
    return new Portal();
});
